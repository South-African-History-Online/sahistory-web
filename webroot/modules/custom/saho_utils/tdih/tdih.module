<?php

/**
 * @file
 * Implements hook_theme() for TDIH submodule.
 */

/**
 * Implements hook_theme().
 */
function tdih_theme($existing, $type, $theme, $path) {
  $module_path = \Drupal::service('extension.list.module')->getPath('tdih');

  return [
    'tdih_block' => [
      'variables' => [
        'nodes_data' => NULL,
      ],
      'template' => 'block--inline-block--tdih',
      'path' => $module_path . '/templates',
    ],
  ];
}
