<?php

/**
 * @file
 * Install, update and uninstall functions for the layout_builder_fix module.
 */

/**
 * Implements hook_install().
 */
function layout_builder_fix_install() {
  \Drupal::messenger()->addStatus(t('Layout Builder Fix module has been installed. This module fixes a type error in the InlineBlock class after updating to Drupal 11.'));
}