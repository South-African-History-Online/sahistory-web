{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 10 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/payfast_donation_block",
                "version": "1.0.1",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupalcode.org/project/payfast_donation_block.git",
                    "type": "git",
                    "reference": "1.0.1"
                },
                "require": {
                    "drupal/core": "^8.8 || ^9 || ^10 || ^11"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "payfast/commerce_payfast",
                "version": "1.5.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/Payfast/drupalcommerce-aggregation.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.3",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.5",
        "drupal/admin_toolbar_tools": "^3.5",
        "drupal/antibot": "^2.0",
        "drupal/asset_injector": "^2.10",
        "drupal/belgrade": "^3.0",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/better_social_sharing_buttons": "^4.0",
        "drupal/bootstrap_layout_builder": "^2.2",
        "drupal/colorbox": "^2.1",
        "drupal/commerce": "^3.0",
        "drupal/commerce_paypal": "^2.0",
        "drupal/commerce_pricelist": "^2.0",
        "drupal/commerce_recurring": "^1.0",
        "drupal/commerce_shipping": "^2.0",
        "drupal/commerce_stripe": "^1.0",
        "drupal/commerce_variation_cart_form": "^1.0",
        "drupal/commerce_wishlist": "^3.0@RC",
        "drupal/config_ignore": "^3.3",
        "drupal/content_lock": "^3.0@alpha",
        "drupal/core-composer-scaffold": "^11",
        "drupal/core-project-message": "^11",
        "drupal/core-recommended": "^11",
        "drupal/core-vendor-hardening": "^11",
        "drupal/ctools": "^4.1",
        "drupal/devel": "^5.5",
        "drupal/dropzonejs": "^2.6",
        "drupal/dynamic_entity_reference": "^4.0@alpha",
        "drupal/enhanced_button_link": "^1.2",
        "drupal/entity_reference_quantity": "^3.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/facets": "^3.0",
        "drupal/field_display_toggle": "^1.0",
        "drupal/field_group": "^4.0",
        "drupal/file_entity": "^2.0@beta",
        "drupal/fontawesome": "^3.0",
        "drupal/geofield": "^1.57",
        "drupal/geolocation": "^4.0@beta",
        "drupal/geolocation_leaflet": "^3.9",
        "drupal/gin": "^5.0",
        "drupal/gin_toolbar": "^3.0",
        "drupal/google_analytics": "^4.0",
        "drupal/httpswww": "^2.1",
        "drupal/imagemagick": "^4.0",
        "drupal/imce": "^3.1",
        "drupal/jquery_deprecated_functions": "^1.0",
        "drupal/jquery_ui": "^1.8",
        "drupal/jquery_ui_draggable": "^2.1",
        "drupal/jquery_ui_resizable": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/js_cookie": "^1.0",
        "drupal/layout_builder_blocks": "^1.1",
        "drupal/layout_builder_modal": "^2.0",
        "drupal/layout_builder_reorder": "^2.0",
        "drupal/leaflet": "^10.2",
        "drupal/linkchecker": "^2.1@alpha",
        "drupal/linkit": "^7.0",
        "drupal/metatag": "^2.1",
        "drupal/pathauto": "^1.10",
        "drupal/payfast_donation_block": "^1.0",
        "drupal/pdf_reader": "^1.1",
        "drupal/profile": "^1.0",
        "drupal/quick_node_clone": "^1.18",
        "drupal/quicktabs": "^4.0",
        "drupal/radix": "^6",
        "drupal/redirect": "^1.7",
        "drupal/redirect_404": "^1.7",
        "drupal/redirect_domain": "^1.7",
        "drupal/redis": "^1.8",
        "drupal/robotstxt": "^1.6",
        "drupal/search_api": "^1.38",
        "drupal/search_api_autocomplete": "^1.10",
        "drupal/search_api_block": "^1.1",
        "drupal/search_api_db": "^1.23",
        "drupal/search_api_solr": "^4.3",
        "drupal/search_api_sort_priority": "^1.10",
        "drupal/seckit": "^2.0",
        "drupal/simple_sitemap": "^4.2",
        "drupal/spammaster": "^2.62",
        "drupal/statistics": "^1.0",
        "drupal/tamper": "^1.0@alpha",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/term_merge": "^2.0@beta",
        "drupal/tvi": "^2.0",
        "drupal/twig_tweak": "^3.4",
        "drupal/ultimate_cron": "^2.0@beta",
        "drupal/views_bulk_operations": "^4.3",
        "drupal/views_data_export": "^1.8",
        "drupal/views_slideshow": "^5.0",
        "drupal/webform": "^6.3@beta",
        "drupal/webform_attachment": "^6.1",
        "drupal/webform_ui": "^6.1",
        "drush/drush": "^13",
        "guzzlehttp/guzzle": "^7",
        "payfast/commerce_payfast": "^1.5",
        "tijsverkoyen/css-to-inline-styles": "^2.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "webroot/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.htaccess": ".htaccess.custom"
            }
        },
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "webroot/drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "webroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "webroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "Issue #2325899: Add option for Views to skip invalid default arguments": "https://www.drupal.org/files/issues/2024-06-24/2325899-212.patch",
                "Issue #3049332: Log error + visual warning for missing or broken block": "patches/layout-builder-inlineblock-logger-interface-drupal11-fixed.patch"
            },
            "drupal/list_inline_block": {
                "Issue #3431706: Automated Drupal 11 compatibility fixes for list_inline_block": "https://www.drupal.org/files/issues/2024-03-18/list_inline_block.1.1.1.rector.patch"
            },
            "drupal/seckit": {
                "Issue #3208781: Add script-src-elem and style-src-elem CSP directives": "https://www.drupal.org/files/issues/2025-01-23/3208781-19.patch"
            },
            "payfast/commerce_payfast": {
                "Drupal 11 compatibility: Add ^11 to core_version_requirement": "patches/commerce_payfast-drupal11-compatibility.patch"
            },
            "drupal/payfast_donation_block": {
                "Drupal 11 compatibility: Add ^11 to core_version_requirement": "patches/payfast_donation_block-drupal11-compatibility.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "webroot/modules/contrib/webform/composer.libraries.json",
                "webroot/modules/contrib/media_directories/composer.libraries.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "mglaman/drupal-check": "^1.5",
        "slevomat/coding-standard": "^8.18",
        "squizlabs/php_codesniffer": "*"
    }
}
