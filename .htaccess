 Options +FollowSymLinks
 RewriteEngine on
 
 RewriteCond %{HTTP_HOST} ^shop\.sahistory\.org\.za$
 RewriteCond %{REQUEST_URI} !^/shop
 RewriteRule ^.*$ https://www\.sahistory\.org\.za/shop [L,R=301]

 RewriteCond %{REQUEST_URI} ^/topic/(.*)$ [NC]
 RewriteRule ^topic/(.*)$ /article/$1 [L,R]
 
 RewriteCond %{REQUEST_URI} ^/places/(.*)$ [NC]
 RewriteRule ^places/(.*)$ /place/$1 [L,R]
 
 RewriteRule ^sites/default/files/images/(.*)$ /sites/default/files/images_new/$1 [L,R]
 
 RewriteRule ^archive/(.*),(.*)$ /archive/$1$2 [L]
 RewriteRule ^archive/(.*):(.*)$ /archive/$1$2 [L]
 RewriteRule ^archive/(.*)\'(.*)$ /archive/$1$2 [L]
 RewriteRule ^archive/(.*)\"(.*)$ /archive/$1$2 [L]
 RewriteRule ^archive/(.*)\.(.*)$ /archive/$1$2 [L]

 RewriteCond %{ENV:REDIRECT_STATUS} 200
 RewriteRule ^archive/(.*)$ /archive/$1 [L,R=301]

 RewriteRule ^dated-event/(.*),(.*)$ /dated-event/$1$2 [L]
 RewriteRule ^dated-event/(.*):(.*)$ /dated-event/$1$2 [L]
 RewriteRule ^dated-event/(.*)\'(.*)$ /dated-event/$1$2 [L]
 RewriteRule ^dated-event/(.*)\"(.*)$ /dated-event/$1$2 [L]
 RewriteRule ^dated-event/(.*)\.(.*)$ /dated-event/$1$2 [L]

 RewriteCond %{ENV:REDIRECT_STATUS} 200
 RewriteRule ^dated-event/(.*)$ /dated-event/$1 [L,R=301]
 
 RewriteCond %{REQUEST_URI} ^/sites/default/files/DC/.*/(.*)$ 
 RewriteCond %{DOCUMENT_ROOT}/sites/default/files/archive-files/$1 -f
 RewriteRule ^sites/default/files/DC/.*/(.*)$ /sites/default/files/archive-files/$1 [L] 
 RewriteCond %{DOCUMENT_ROOT}/sites/default/files/archive-files2/$1 -f
 RewriteRule ^sites/default/files/DC/.*/(.*)$ /sites/default/files/archive-files2/$1 [L]
 RewriteCond %{DOCUMENT_ROOT}/sites/default/files/archive-files3/$1 -f
 RewriteRule ^sites/default/files/DC/.*/(.*)$ /sites/default/files/archive-files3/$1 [L]
 RewriteCond %{DOCUMENT_ROOT}/sites/default/files/archive-files4/$1 -f
 RewriteRule ^sites/default/files/DC/.*/(.*)$ /sites/default/files/archive-files4/$1 [L]
 
